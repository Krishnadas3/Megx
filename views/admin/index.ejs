      <script>
        $('#myDataTable')
          .addClass('nowrap')
          .dataTable({
            responsive: true,
            columnDefs: [
              { targets: [-1, -3], className: 'dt-body-right' }
            ]
          });
      </script>

<script>
  // Order Statistics Chart
  const orderStatisticsCtx = document.getElementById('orderStatisticsChartCanvas').getContext('2d');
  const orderStatisticsChart = new Chart(orderStatisticsCtx, {
      type: 'doughnut',
      data: {
          labels: ['Delivered', 'Confirmed', 'Returned', 'Cancelled'],
          datasets: [{
              label: 'Orders',
              data: [<%= deliveryCount %>, <%= confirmedCount %>, <%= returnedCount %>, <%= cancelledCount %>],
              backgroundColor: ['#28a745', '#007bff', '#dc3545', '#6c757d']
          }]
      },
      options: {
  legend: {
    labels: {
      usePointStyle: true,
      boxWidth: 6
    }
  }
}
  });

  // Sales Chart
  const salesCtx = document.getElementById('incomeChartCanvas').getContext('2d');
  const salesData = JSON.parse(document.getElementById('sales').value);
  const dateLabels = JSON.parse(document.getElementById('date').value);
  const incomeChart = new Chart(salesCtx, {
      type: 'line',
      data: {
          labels: dateLabels,
          datasets: [{
              label: 'Sales',
              data: salesData,
              borderColor: '#007bff',
              backgroundColor: 'rgba(0, 123, 255, 0.1)'
          }]
      },
      options: {
          responsive: true,
          scales: {
              y: {
                  beginAtZero: true
              }
          }
      }
  });
</script>
      

