//       location.href = '/admin/list-order'

//     }
//   }
// })

// }
    </script>

    <!-- Jquery Core Js -->
    <script src="/assets/admin/assets/bundles/libscripts.bundle.js"></script>

    <!-- Jquery Plugin -->
    <script src="/assets/admin/assets/plugin/nouislider/nouislider.min.js"></script>

    <!-- Jquery Page Js -->
    <script src="../js/template.js"></script>

    <!-- Jquery Page Js -->
    <script src="../js/template.js"></script>
    <script>
        $('#myDataTable')
        .addClass( 'nowrap')
        .dataTable( {
            responsive: true,
            columnDefs: [
                { targets: [-1, -3], className: 'dt-body-right' }
            ]
        });
    </script>
</body>

