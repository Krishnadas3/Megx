        <script>
           const plusBtn = document.querySelector('.plus-btn');
    const minusBtn = document.querySelector('.minus-btn');
    const quantityDisplay = document.querySelector('.quantity');
    const subtotalElement = document.querySelector('.subtotal');

    // Get the product price
    const price = parseFloat(document.querySelector('.amount').textContent.replace('$', ''));

    // Add event listener for plus button click
    plusBtn.addEventListener('click', function() {
        // Get the current quantity
        let quantity = parseInt(quantityDisplay.textContent);
        // Increment the quantity
        quantity++;
        // Update the quantity display
        quantityDisplay.textContent = quantity;

        // Update the subtotal based on the new quantity and price
        const subtotal = price * quantity;
        // Update the subtotal display
        subtotalElement.textContent = `$${subtotal.toFixed(2)}`;
    });

    // Add event listener for minus button click
    minusBtn.addEventListener('click', function() {
        // Get the current quantity
        let quantity = parseInt(quantityDisplay.textContent);
        // Ensure quantity doesn't go below 1
        if (quantity > 1) {
            // Decrement the quantity
            quantity--;
            // Update the quantity display
            quantityDisplay.textContent = quantity;

            // Update the subtotal based on the new quantity and price
            const subtotal = price * quantity;
            // Update the subtotal display
            subtotalElement.textContent = `$${subtotal.toFixed(2)}`;
        }
    });


  
    // Calculate cart total price and grand total
    function calculateTotal() {
        let cartTotal = 0;
        const productSubtotals = document.querySelectorAll('.subtotal');
        productSubtotals.forEach(subtotal => {
            cartTotal += parseFloat(subtotal.textContent.replace('$', ''));
        });
        document.getElementById('cartTotalPrice').textContent = '$' + cartTotal.toFixed(2);

        // Assuming shipping cost is fixed at $20
        const shippingCost = 20;
        const grandTotal = cartTotal + shippingCost;
        document.getElementById('grandTotalPrice').textContent = '$' + grandTotal.toFixed(2);
    }

    // Call calculateTotal function when the page is loaded
    window.onload = calculateTotal;
</script>
        





